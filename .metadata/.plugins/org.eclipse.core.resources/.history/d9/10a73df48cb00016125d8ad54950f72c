package step_H;

import java.util.Random;
import java.util.Scanner;

public class H02 {
	void run(){
		Scanner s = new Scanner(System.in);
		Random r = new Random();
		int[] lotto = new int[6];
		int count;
		String onemore;
		int i;
		while(onemore == "Y"){
			for(i = 0; i < lotto.length; i++){
				lotto[i] = r.nextInt(45) + 1;
			}
		}
	}
}
