package student_information_management;

import java.util.Scanner;

public class Person {
	Scanner s = new Scanner(System.in);
	String name;
	String sex;
	String address;
	String tel;
	private Person(String name, String studentid, String sex, String address, String tel, Boolean backtoschool){
		name = "";
		sex = "";
		address = "";
		tel = "";
	}
}
