package student_information_management;

public class Run extends StudentManager {

	public Run(String name, String studentid, String gender, String address, String tel, boolean backtoschool) {
		super(name, studentid, gender, address, tel, backtoschool);
	}

	public static void main(String[] args, String name, String studentid, String gender, String address, String tel, boolean backtoschool) {
		Run t = new Run(name, studentid, gender, address, tel, backtoschool);
		t.start();
	}
}
