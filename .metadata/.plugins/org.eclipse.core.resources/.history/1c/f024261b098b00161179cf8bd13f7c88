package step_G;

import java.util.Scanner;

public class G03 {
	void run(){
		Scanner s = new Scanner(System.in);
		int[][] jumsu = new int[5][3];
		int[] c_average = new int[3];
		String[] dass_name = {"국어", "영어", "수학"};
		int kor = 0;
		int eng = 0;
		int mat = 0;
		int sum = 0;
		int average = 0;
		int grade = 0;
		int i = 0;
		int j = 0;
		for(i = 0; i < 5; i++){
			System.out.print((i+1) + "번 학생의 국어, 영어, 수학 점수는? ");
			kor = s.nextInt();
			eng = s.nextInt();
			mat = s.nextInt();
			jumsu[i][0] = kor;
			jumsu[i][1] = eng;
			jumsu[i][2] = mat;
			for(j = 0; j < 3; j++){
				jumsu[i][j]
			}
		}
	}
}
