package lab_08_02;

class TimerThread extends Thread{
	int[] n;
	public TimerThread(int[] one){
		this.n = one;
	}
	public void run(){
		int i = 0;
		int[] one = {1, 2, 3, 4, 5, 6, 7, 8};
		while(i < one.length){
			System.out.println(one[i]);
			i++;
			try{
				Thread.sleep(1000);
			}
		}
	}
	public TimerThread(int[] ten){
		this.n = ten;
	}
	public void run(){
		int i = 0;
		int[] ten = {10, 20, 30, 40, 50};
		while(i < one.length){
			System.out.println(one[i]);
			i++;
			try{
				Thread.sleep(1000);
			}
		}
	}
}
