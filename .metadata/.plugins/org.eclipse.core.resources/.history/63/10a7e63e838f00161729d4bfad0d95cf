package student_information_management;

import java.util.Scanner;

public class Person {
	Scanner s = new Scanner(System.in);
	String name;
	String sex;
	String address;
	String tel;
	public String getName(){
		return name;
	}
	public void setName(String name){
		return this.name;
	}
	private Person(){
		name = "";
		sex = "";
		address = "";
		tel = "";
	}
}
